{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Login User Schema",
  "required": ["phone", "password"],
  "properties": {
    "phone": {
      "type": "string",
      "title": "Phone",
      "minLength": 8,
      "maxLength": 15
    },
    "password": {
      "title": "Password",
      "type": "string",
      "minLength": 8,
      "maxLength": 50
    }
  }
}
