{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "Login User Schema",
    "required": [
        "email",
        "password"
    ],
    "properties": {
        "email": {
            "type": "string",
            "title": "Email",
            "minLength": 3,
            "maxLength": 50
        },
        "password": {
            "title": "Password",
            "type": "string",
            "minLength": 8,
            "maxLength": 50
        }
    }
}